<h2>Instrument editor</h2>
<select id="instrumentSelect">
    <option value="0">Instrument 1</option>
    <option value="1">Instrument 2</option>
</select>
<ul id="oscillatorTabs">
    <li data-oscillator="1">
        Oscillator 1
    </li>
    <li data-oscillator="2">
        Oscillator 2
    </li>
    <li data-oscillator="3">
        Oscillator 3
    </li>
</ul>
<div id="canvasContainer"><!-- x --></div>
<select id="oscillatorEnabled">
    <option value="true">Enabled</option>
    <option value="false">Disabled</option>
</select>
<select id="oscillatorWaveformSelect">
    <option value="SAW">Sawtooth</option>
    <option value="TRIANGLE">Triangle</option>
    <option value="SQUARE">Square</option>
    <option value="CUSTOM">Custom</option>
</select>
<div class="wrapper input range">
    <label for="detune">Detune</label>
    <input type="range" name="tempo" id="detune" min="-50" max="50" step=".1" value="0">
</div>
<div class="wrapper input range">
    <label for="octaveShift">Octave shift</label>
    <input type="range" name="tempo" id="octaveShift" min="-2" max="2" step="1" value="0">
</div>
<div class="wrapper input range">
    <label for="fineShift">Fine shift</label>
    <input type="range" name="tempo" id="fineShift" min="-7" max="7" step="1" value="0">
</div>
